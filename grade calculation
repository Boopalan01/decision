#include <stdio.h>

int main()
{
int mark;
printf("Enter mark: ");
scanf("%d",&mark);
if(mark>84 && mark<=100)
printf("Grade A");
else if(mark>=70 && mark<85)
printf("Grade B");
else if(mark>=55 && mark<70)
printf("Grade C");
else if(mark>=40 && mark<55)
printf("Grade D");
else
printf("Grade F");
}
